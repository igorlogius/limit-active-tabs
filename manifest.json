{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_area": "navbar",
    "theme_icons": [
      {
        "light": "dark-128.png",
        "dark": "light-128.png",
        "size": 128
      }
    ]
  },
  "commands": {
    "_execute_page_action": {
      "suggested_key": {
        "default": "Shift+Alt+L"
      }
    }
  },
  "description": "Limits the number of active tabs, by unloading older tabs (determined by last tab activation)",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Limit Active Tabs",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": ["storage", "tabs"],
  "version": "1.2.9"
}
