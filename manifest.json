{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/limit-active-tabs",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_area": "navbar"
  },
  "commands": {
    "_execute_page_action": {
      "suggested_key": {
        "default": "Shift+Alt+L"
      }
    }
  },
  "description": "Limits number of active tabs, unloading older (determined by last activation)",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Limit Active Tabs",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": ["storage", "tabs"],
  "version": "1.2.11"
}
